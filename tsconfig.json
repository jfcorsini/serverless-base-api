{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "strict": true,
    "baseUrl": "./",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "node",
      "aws-lambda",
    ],
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "resolveJsonModule": true
  },
  "include": [
    "./index.ts"
  ],
  "outDir": "build",
}